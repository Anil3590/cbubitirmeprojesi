<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".muhfakgenel">

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <Button
            android:id="@+id/button4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="muhendislik"
            android:text="KONUM" />

        <Button
            android:id="@+id/btnmuharama"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="İLETİŞİM" />

        <Button
            android:id="@+id/btnmuhweb"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="WEB SİTESİ" />

        <Button
            android:id="@+id/button6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="MuhAkademik"
            android:text="Akademik Kadro" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>